package arrayListAssignmnt2;
import java.util.*;
import java.io.*;
class Employee
{
	private int empid;
	private String name, designation;
	private double salary;
	
	Employee(int empid, String name, String designation, double salary)
	{
		this.empid = empid;
		this.name = name;
		this.designation = designation;
		this.salary = salary;
	}
	public String toString()
	{
		return "empid  [empid "+empid + "name [name "+name+"designation [designation"+designation+"salary [salary"+salary+"]";
	}
}

public class EmployeeDemo 
{

	public static void main(String[] args) 
	{
		Employee e = new Employee(1, "Akshay", "Manager",30.00);
		Employee e2 = new Employee(2, "Tripti", "S.Manager",40.00);
		Employee e3 = new Employee(3, "Kanika", "Associate",50.00);
		Employee e4 = new Employee(4, "Trisha", "S.Associate",60.00);
		
		List<Employee>myList = new ArrayList<>();
		myList.add(e);
		myList.add(e2);
		myList.add(e3);
		myList.add(e4);
		
		
		
	}

}
