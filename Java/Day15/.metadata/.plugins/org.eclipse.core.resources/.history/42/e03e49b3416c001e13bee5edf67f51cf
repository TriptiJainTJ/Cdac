package q1;
interface Seventh
{
	void fun(int k);
}
class Sample
{
	Sample(int k)
	{
		System.out.println("inside parameterized constructor of Sample "+k);
	}
}
public class q1 
{

	public static void main(String[] args) 
	{
		Sixth ref = Sample::fun1;
		ref.disp5();
		//lambda
//		Sixth ref =()->{Sample.fun1();};
//		ref.disp5();

	}

}